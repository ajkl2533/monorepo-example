{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "build-storybook"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{workspaceRoot}/dist"]
    },
    "build-storybook": {
      "dependsOn": ["build"],
      "outputs": ["{workspaceRoot}/storybook-static"]
    }
  }
}
